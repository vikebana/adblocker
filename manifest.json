{
    "manifest_version": 3,
    "name": "LinkedIn AdBlocker",
    "description": "Blocking ads.",
    "version": "0.0.1",
    "author": "<AUTHOR_NAME>",
    "action": {
        "default_title": "LinkedIn AdBlocker"
    },
    "host_permissions": [
        "https://www.linkedin.com/"
    ],
    "permissions": [
        "tabs",
        "webNavigation",
        "scripting"
    ],
    "background": {
        "service_worker": "extension.js"
    }
}